@model List<Capstone.Web.Models.ParkModel>
@{
    ViewBag.Title = "ChoosePark";
}


<h2>Choose your park</h2>


<div id="map">
    
</div>
    
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDMTlmooQjxAlorbRMikqwVUu2nYApFy5Y&callback=initMap">
    </script>



<div class="park-list-description">

    @foreach (var park in Model)
    {
        <div class="inner-box-park">
        <img src="~/Content/images/@park.Image" />
        <h4>@park.Name</h4>
        <span>@park.Description</span>
        </div>
    }

</div>