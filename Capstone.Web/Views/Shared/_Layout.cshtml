<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>


</head>
<body>

    <header>
        <div class="left">
            <img src="~/Content/images/MetroParkLogo.jpg" width="500px" height="300px" />
        </div>
        <div class="right">
            <span>TEAM</span><span>&amp;</span>
        </div>
    </header>

    <nav>
        @Html.Partial("_LoginPartial")
    </nav>

    <main>
        @RenderBody()
    </main>

    @RenderSection("scripts", required: false)

    <footer>&copy; @DateTime.Now.Year TeamAmpersand</footer>
</body>
</html>
